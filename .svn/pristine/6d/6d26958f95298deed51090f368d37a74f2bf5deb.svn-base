public class ItsDTitem {
    private int dynamicId;//宠物记录ID
    private int petUserId;//宠物ID
    private String  petUserImg;//宠物头像
    private String petUserName;//宠物名
    private String PetUserTitle;//宠物等级
    private String dyImg;//宠物动态主图
    private String petLeave;//宠物留言

    public ItsDTitem(int dynamicId){
        this.dynamicId=dynamicId;
        this.petUserImg="res://.../" + R.drawable.cw_touxiang;
        this.petUserName="蓝宝吾皇";
        this.PetUserTitle="十八线名宠";
        this.dyImg = "res://.../" + R.drawable.img0;
        this.petLeave = ItsPetCommon.ItsRM.ITS_RM_YAN+"蓝宝吾皇今日早上微服私访七品铲屎官的卧室，竟然发现铲屎官在偷吃朕的小鱼干！"+dynamicId;
    }

    public int getDynamicId() {
        return dynamicId;
    }

    public void setDynamicId(int dynamicId) {
        this.dynamicId = dynamicId;
    }

    public int getPetUserId() {
        return petUserId;
    }

    public void setPetUserId(int petUserId) {
        this.petUserId = petUserId;
    }

    public String getPetUserImg() {
        return petUserImg;
    }

    public void setPetUserImg(String petUserImg) {
        this.petUserImg = petUserImg;
    }

    public String getPetUserName() {
        return petUserName;
    }

    public void setPetUserName(String petUserName) {
        this.petUserName = petUserName;
    }

    public String getPetUserTitle() {
        return PetUserTitle;
    }

    public void setPetUserTitle(String petUserTitle) {
        PetUserTitle = petUserTitle;
    }

    public String getDyImg() {
        return dyImg;
    }

    public void setDyImg(String dyImg) {
        this.dyImg = dyImg;
    }

    public String getPetLeave() {
        return petLeave;
    }

    public void setPetLeave(String petLeave) {
        this.petLeave = petLeave;
    }
}
